<resources>    <!-- Base application theme. -->    <style name="AppTheme" parent="Theme.AppCompat.NoActionBar">        <!-- Customize your theme here. -->        <item name="android:windowFullscreen">true</item>    </style>    <style name="CodeFont" parent="android:Theme.NoTitleBar.Fullscreen">        <item name="android:windowNoTitle">true</item>    </style>    <style name="FullScreen" parent="Theme.AppCompat.NoActionBar">        <item name="android:windowFullscreen">true</item>        <item name="android:windowNoTitle">true</item>    </style>    <style name="BottomSheet" parent="@style/Widget.Design.BottomSheet.Modal">        <item name="android:background">@drawable/bg_bottom_sheet_dialog</item>    </style>    <style name="BaseBottomSheetDialog" parent="@style/Theme.Design.Light.BottomSheetDialog">        <item name="android:windowIsFloating">false</item>        <item name="bottomSheetStyle">@style/BottomSheet</item>    </style>    <style name="BottomSheetDialogTheme" parent="BaseBottomSheetDialog" />    <style name="radioTheme" parent="Theme.AppCompat.Light">        <item name="colorControlActivated">@color/cornflowerBlue</item>    </style>    <style name="HintTextAppearance" parent="TextAppearance.Design.Hint">        <item name="android:textColor">@drawable/hint_color</item>    </style>    <style name="AppTheme.TextInput" parent="Widget.Design.TextInputLayout">        <item name="hintTextAppearance">@style/HintTextAppearance</item>        <item name="android:layout_width">match_parent</item>        <item name="android:layout_height">wrap_content</item>    </style>    <style name="PoppinsRegularAppearance" parent="android:TextAppearance">        <item name="fontFamily">fonts/poppins_regular.ttf</item>    </style>    <style name="MyTheme.EditText" parent="Theme.AppCompat.Light">        <item name="colorControlNormal">@color/blue_unicid</item>        <item name="colorControlActivated">@color/blue_unicid</item>        <item name="android:textAppearance">@style/PoppinsRegularAppearance</item>        <item name="android:textSize">16sp</item>    </style></resources>